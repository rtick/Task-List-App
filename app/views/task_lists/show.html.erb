<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @task_list.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @task_list.description %>
</p>

<ul>
	<% @task_list.tasks.each do |task|%>
		<li>
			<%= task.title %>
		</li>
	<% end %>
</ul>

<%= link_to 'Edit', edit_task_list_path(@task_list) %> |
<%= link_to 'Back', task_lists_path %>
